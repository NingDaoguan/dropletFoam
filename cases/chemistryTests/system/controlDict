FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     chemFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e-2;

deltaT          1e-9;

adjustTimeStep  on;

writeControl    adjustableRunTime;

writeInterval   1e-6;

purgeWrite      0;

writeFormat     binary;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

suppressSolverInfo yes;

maxDeltaT       1e-6;


