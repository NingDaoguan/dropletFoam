FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      VOFSetFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultSpecies
(
    N2   1.0
);

vaporPhaseName   alphaVapor;

shapes
(
    DropMMH
    {
        alphaName alphaLiquid;
        
        shape    ellipsoid;
        
        ellipsoidCoeffs
        {
            center   (0 0 0.00415);
            radius   (0.00115 0.00115 0.00115);
        }
        
        delVapor 3e-4;
        U        (0 0 -1.3);
        
        liquidSpecies
        (
            CH3NHNH2L  1.0
        );
        
        vaporSpecies
        (
            CH3NHNH2   0.05
        );
    }
    
    DropRFNA
    {
        alphaName alphaLiquid;
        
        shape    ellipsoid;
                
        ellipsoidCoeffs
        {
            center   (0 0 0);
            radius   (0.0035 0.0035 0.002);
        }
        
        delVapor 6e-4;
        U        (0 0 0);
        
        liquidSpecies
        (
            RFNAL  1.0
        );
        
        vaporSpecies
        (
            RFNA   0.15
        );
    }

);


