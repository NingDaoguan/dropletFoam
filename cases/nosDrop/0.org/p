FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 4.5e5;

boundaryField
{
    atmosphere
    {
        type            calculated;
        value           $internalField;
    }
    
    inflow
    {
        type            calculated;
        value           $internalField;
    }

    symmetry
    {
        type            symmetryPlane;
    }
    
    walls
    {
        type            calculated;
        value           $internalField;
    }
    
    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
