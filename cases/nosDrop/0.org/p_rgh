FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 4.5e5;

boundaryField
{
    atmosphere
    {
        type            fixedValue;
        value           uniform 4.5e5;
        
        /*type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1;
        value           uniform 0;*/
    }
    
    inflow
    {
        //type            zeroGradient;
        //type            fixedFluxPressure;
        //value           uniform 0;
        type            fixedValue;
        value           uniform 450005;
        
        /*type            uniformFixedValue;
        uniformValue    table
        (
          (0     450003)
          (0.012 450003)
          (0.013 450001)
          (10    450001)
        );*/
    }

    walls
    {
        type            buoyantPressure;
        value           uniform 4.5e5;
    }
    
    symmetry
    {
        type            symmetryPlane;
    }
    
    defaultFaces
    {
        type            empty;
    }
}

