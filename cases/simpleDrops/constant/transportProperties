FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Air
phase1
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0] 1.48e-05;
    rho             rho [1 -3 0 0 0 0 0] 1.2;
}

// MMH
phase2
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0]  9.2e-7; //rocketdyne
    rho             rho [1 -3 0 0 0 0 0] 870;    //Schmidt
}

// RFNA
phase3
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0]  7.94e-7; //NASA MON document
    rho             rho [1 -3 0 0 0 0 0] 1550;    //NASA MON document
}

// Surface tension coefficients
sigma12           sigma12 [1 0 -2 0 0 0 0] 0.0338;  //Schmidt p 257 at 298 K
sigma13           sigma13 [1 0 -2 0 0 0 0] 0.04013; //NASA MON document at 298 K

// Diffusivity between miscible phases
D23               D23   [0 2 -1 0 0 0 0]  1e-09; //unknown, assumed Sc ~ 1000 based on other liquids

// ************************************************************************* //
